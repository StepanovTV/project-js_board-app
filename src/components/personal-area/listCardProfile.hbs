{{#each this}}
<li class="cardProfile" id="{{_id}}">
    <img src="{{images}}" alt="первое фото товара" class="startImgAd"
        width="150" />
    <div class="miniInfoAd">
        
        <p class="miniInfotitle">
            {{title}}
        </p>
        <button class="editId" id="{{_id}}" data-action="edit">Edit</button>
        <p class="miniInfoPrice">
            
        </p>
        <p class="miniInfodescription">
            {{description}}
        </p>
        <p class="miniInfoPrice">
           Вартість: {{price}}
        </p>
    </div>
    <button class="dellIdAd" id="{{_id}}" data-action="delete">X</button>
</li>
{{/each}}
