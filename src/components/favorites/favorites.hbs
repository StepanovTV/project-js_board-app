
    <h2 class="favorites-title">Мої Обрані</h2>
    <ul class="favoritList">
        {{#each this}}
        <li class="cardProfile adsItem" data-adid="{{_id}}">
            <img src="{{images}}" alt="первое фото товара" class="startImgAd" width="150" />
            <div class="miniInfoAd">

                <p class="miniInfotitle">
                    {{title}}
                </p>

                <p class="miniInfoPrice">
                    Вартість: {{price}}
                </p>
            </div>
        </li>
        {{/each}}
    </ul>
